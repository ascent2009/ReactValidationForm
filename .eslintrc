{
  "env": {
    "browser": true
  },
  "extends": ["react-app", "prettier", "prettier/react"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["prettier", "react"],
  "rules": {
    "react/prop-types": "off",
    "no-shadow": "warn",
    "no-console": "warn",
    "react/jsx-filename-extension": ["error", { "extensions": [".jsx", ".js"] }],
    "import/extensions": [
      "error",
      "ignorePackages",
      {
        "js": "never",
        "jsx": "never"
      }
    ]
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "webpack.config.js",
      }
    }
  }
}
